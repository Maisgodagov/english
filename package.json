{
  "name": "english-platform",
  "private": true,
  "version": "0.1.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev:mobile": "npm run start --workspace @english/mobile",
    "android": "npm run android --workspace @english/mobile",
    "ios": "npm run ios --workspace @english/mobile",
